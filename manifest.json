{
    "manifest_version": 2,
    "name": "QR Code Generator",
    "version": "1.0",
    "description": "Generate QR codes from text or clipboard with a customizable UI and save as PNG.",
    "icons": {
      "48": "favicon.png"
        },
        "permissions": [
          "activeTab"
      ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "QR Code Generator"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "com.gifmakes.qrcodegenerator@8a634f32-1650-4181-a5cf-55c5779ec3ff",
        "strict_min_version": "109.0"
      }
    }
}